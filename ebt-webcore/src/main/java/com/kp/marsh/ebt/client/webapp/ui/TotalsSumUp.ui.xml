<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:com.kp.marsh.ebt.client.webapp.ui">
	<ui:with field="resources" type="com.kp.marsh.ebt.client.webapp.ui.resources.Resources"></ui:with>

	<g:HTMLPanel>
		<div class="{resources.mainStructure.subtotalBox}" ui:field="subtotalsDiv">
			<table border="0" cellpadding="0" cellspacing="0"
				class="{resources.mainStructure.selecTotal}">
				<tr ui:field="potSum">
					<td class="{resources.mainStructure.potBulLabel}">
						<g:RadioButton name="type" checked="true" ui:field="radioPot">Potenziale</g:RadioButton>
					</td>
					<td class="{resources.mainStructure.potBulValue}" ui:field="cePotTotal" />
				</tr>
				<tr ui:field="achSum">
					<td class="{resources.mainStructure.achBulLabel}">
						<g:RadioButton name="type" ui:field="radioAch">Achieved</g:RadioButton>
					</td>
					<td class="{resources.mainStructure.achBulValue}" ui:field="ceAchTotal" />
				</tr>
			</table>
		</div>
		<!-- elenco delle linee di business -->
		<div class="{resources.mainStructure.titleBox2}" ui:field="headlinesTableDiv" />
	</g:HTMLPanel>
</ui:UiBinder> 