<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:com.kp.marsh.ebt.client.webapp.ui">


	<ui:with field="resources" type="com.kp.marsh.ebt.client.webapp.ui.resources.Resources"></ui:with>
	<ui:style>
		.dialogBox {
			width: 400px;
			height: 400px;
			background-color: #333333;
			color: #ffffff;
			font-size: 0.8em;
		}
	</ui:style>


	<g:HTMLPanel styleName="{resources.mainStructure.widgetBox}">
		<table border="0" cellpadding="0" cellspacing="0"
			class="{resources.mainStructure.totalWidget}">
			<tr>
				<td colspan="2" class="{resources.mainStructure.clientLabel}">
					<g:Label ui:field="rowName"></g:Label>
				</td>
				<td class="{resources.mainStructure.trashIcon}">
					<g:Image ui:field="trashOn" resource="{resources.trashOn}" />
					<g:Image ui:field="trashOff" resource="{resources.trashOff}" />
				</td>
			</tr>
			<tr>
				<td class="{resources.mainStructure.achWidgLabel}">Achieved</td>
				<td class="{resources.mainStructure.achWidgValue}">
					<g:Label ui:field="totAch" />
				</td>
			</tr>
			<tr>
				<td class="{resources.mainStructure.potWidgLabel}">Potenziale</td>
				<td class="{resources.mainStructure.potWidgValue}">
					<g:Label ui:field="totPot" />
				</td>
			</tr>
			<tr ui:field="conRow">
				<td class="{resources.mainStructure.consWidgLabel}">Anno precedente</td>
				<td class="{resources.mainStructure.consWidgValue}">
					<g:Label ui:field="totCon" />
				</td>
			</tr>
		</table>


		<table border="0" cellpadding="0" cellspacing="0"
			class="{resources.mainStructure.areaWidget}">
			<tr>
			</tr>
			<thead class="{resources.mainStructure.lineaBus}">
				<tr>
					<th>
						<g:HTML ui:field="label_1" />
					</th>
					<th>
						<g:HTML ui:field="label_2" />
					</th>
					<th>
						<g:HTML ui:field="label_3" />
					</th>
					<th>
						<g:HTML ui:field="label_4" />
					</th>
					<th>
						<g:HTML ui:field="label_5" />
					</th>
					<th>
						<g:HTML ui:field="label_6" />
					</th>
					<th>
						<g:HTML ui:field="label_7" />
					</th>
					<th>
						<g:HTML ui:field="label_8" />
					</th>
				</tr>
			</thead>
			<tbody class="{resources.mainStructure.workArea}">
				<tr>
					<td>
						<r:ProductPillWidget ui:field="pill_1" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_2" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_3" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_4" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_5" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_6" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_7" />
					</td>
					<td>
						<r:ProductPillWidget ui:field="pill_8" />
					</td>
				</tr>
			</tbody>
		</table>
	</g:HTMLPanel>

</ui:UiBinder> 