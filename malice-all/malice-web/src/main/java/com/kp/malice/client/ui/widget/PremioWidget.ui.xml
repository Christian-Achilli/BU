<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.dom.client">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.destra {
			text-align: right;
		}
		
		.labelColumn {
			width: 14.28571%;
			color: #777;
			text-transform: uppercase;
		}
		
		.tablePremi {
			width: 100%;
		}
		
		.bold {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<table class="{style.tablePremi}">
			<tr>
				<td class="{style.labelColumn}">RISCHIO</td>
				<td class="{style.labelColumn}">PREMIO NETTO</td>
				<td class="{style.labelColumn}">ACCESSORI</td>
				<td class="{style.labelColumn}">TASSE</td>
				<td class="{style.labelColumn}">PREMIO LORDO</td>
				<td class="{style.labelColumn}">COMM. SU NETTO</td>
				<td class="{style.labelColumn}">COMM. SU ACC.</td>
			</tr>
			<tr>
				<td>
					<span ui:field="rischio" />
				</td>
				<td>
					<span ui:field="premioNetto" />
				</td>
				<td>
					<span ui:field="accessori" />
				</td>
				<td>
					<span ui:field="tasse" />
				</td>
				<td>
					<span ui:field="premioLordo" class="{style.bold}" />
				</td>
				<td>
					<span ui:field="commissioniSuNetto" />
				</td>
				<td>
					<span ui:field="commissioniSuAccessori" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>