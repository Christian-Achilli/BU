	xmlns:d="urn:import:com.kp.malice.client.ui">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.section {
			margin-left: 7%;
			font-size: 0.75em;
			width: 840px;
			float: left;
		}
	</ui:style>
	<g:HTMLPanel>
		<h:GiornataToolbarAggregati ui:field="toolbarIncassiAggregati" />
		<div class="{resources.main.contentContainer}">
			<div class="{style.section}">
				<h:GiornataListaIncassiWidget ui:field="listaIncassiAggregati" />
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 