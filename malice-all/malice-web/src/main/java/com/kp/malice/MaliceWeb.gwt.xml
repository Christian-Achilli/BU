<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='malice'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />
	<inherits name="org.adamtacy.GWTEffects" />
	<inherits name="pl.rmalinowski.gwt2swf.GWT2SWF" />
	<inherits name="org.vaadin.gwtgraphics.GWTGraphics" />
	<inherits name="com.google.gwt.i18n.I18N" />
    <inherits name='com.google.gwt.xml.XML' />


	<extend-property name="locale" values="it" />

	<inherits
		name='com.google.web.bindery.requestfactory.RequestFactory' />
	<inherits name="com.google.gwt.http.HTTP" />
	
	<!-- Inheriting 'Debug' on the next line makes the features available. -->
   <inherits name='com.google.gwt.debug.Debug'/>
   
   <!-- Disable for production by uncommenting the next line -->
   <!-- <set-property name="gwt.enableDebugId" value="false"/> -->

	<entry-point class='com.kp.malice.client.MaliceWeb' />

	<!-- Specify the paths for translatable code -->
	<source path="client" />
	<source path="shared" />

</module>