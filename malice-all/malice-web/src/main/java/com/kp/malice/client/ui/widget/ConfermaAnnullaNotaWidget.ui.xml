	<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:t="urn:import:com.kp.malice.client.ui.widget" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.textArea {
			float: left;
			height: 31px;
			margin-left: 15px;
			width: 565px;
		}
	</ui:style>
	<g:HTMLPanel>
		<table class="{resources.main.tabellaDivComparsa}">
			<tr>
				<td>Nota:</td>
				<td>
					<g:TextArea ui:field="note" stylePrimaryName="{style.textArea}" />
				</td>
			</tr>
		</table>
		<div class="{resources.main.separatore}" ui:field="divSeparatore" />
		<div class="{resources.main.smallButtonsContainer}">
			<t:AnnullaConfermaWidget ui:field="annullaConfermaWidget" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder>