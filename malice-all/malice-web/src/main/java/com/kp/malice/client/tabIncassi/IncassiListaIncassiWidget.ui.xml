<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:k="urn:import:com.kp.malice.client.ui.resources" xmlns:f="urn:import:com.kp.malice.client.ui.widget">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.myTable {
			margin-top: 7px;
			float: left;
		}
		
		.myTable td {
			padding-right: 5px;
		}
		
		.font {
			font-size: 0.95em;
			margin-bottom: 0px;
			margin-right: 0px;
		}
		
		.listaTitoli {
			margin-top: 20px;
		}
		
		.divPaginatoreDidascalia {
			background-color: #F6F6F6;
			height: 40px;
				padding-left: 10px;
			padding-right: 10px;
			width: 820px;
		}
	</ui:style>
	<g:HTMLPanel>
		<c:CellTable ui:field='cellTable' />
		<div class="{style.divPaginatoreDidascalia}">
			<k:SimplePagerMalice stylePrimaryName='{style.myTable}'
				ui:field='pager' />
		</div>
	</g:HTMLPanel>
</ui:UiBinder>