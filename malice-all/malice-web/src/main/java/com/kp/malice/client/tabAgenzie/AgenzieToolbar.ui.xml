<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.benvenuto {
			float: left;
			margin-left: 50%;
			width: 49%;
			margin-top: 20px;
		}
		
		.benvenuto h1 {
			margin-left: -50%;
			color: #565656;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="{resources.main.toolbarContainer}">
			<div class="{resources.main.toolsContainer}">
				<div class="{resources.main.separatore}  {resources.main.floatLeft}" />
				<div class="{resources.main.firstBoxToolbar}">
					<h1>Agenzie Trovate</h1>
					<g:Label ui:field='tolaleElementiTrovati' />
				</div>
				<div class="{resources.main.separatore}" />
				<div class="{style.benvenuto}">
					<h1>Benvenuto SUPER AGENTE, seleziona l'agenzia di acesso
						desiderata:</h1>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 