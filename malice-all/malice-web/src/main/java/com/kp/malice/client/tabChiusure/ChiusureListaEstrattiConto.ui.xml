<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
 xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:k="urn:import:com.kp.malice.client.ui.resources">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.myTable {
			margin-top: 7px;
			font-size: 0.7em;
			float: left;
		}
		
		.myTable td {
			padding-right: 5px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:ScrollPanel>
			<g:HTMLPanel>
				<div class="{resources.main.tableContainer}  {resources.main.height488}">
					<c:CellTable ui:field='cellTable' />
				</div>
				<div class="{resources.main.divPaginatoreDidascalia}">
					<div>
						<k:SimplePagerMalice stylePrimaryName='{style.myTable}'
							ui:field='pager' />
					</div>
				</div>
			</g:HTMLPanel>
		</g:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 