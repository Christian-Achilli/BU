<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:f="urn:import:com.kp.malice.client.ui.commonWidgets" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:com.kp.malice.client.ui" xmlns:t="urn:import:com.kp.malice.client.ui.toolbar">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.msgIncassiZero {
			margin-top: 20px;
            margin-left: 7%;
            font-size: 0.75em;
            float: left;
		}
	</ui:style>
	<g:HTMLPanel>
		<t:ToolbarScritturaContabile ui:field="toolbarScritturaContabile" />
		<div class="{resources.main.contentContainer}">
			<g:Label ui:field="msgIncassiZero" stylePrimaryName="{style.msgIncassiZero}" />
			<f:ListaScritturaContabileWidget
				ui:field="listaScritturaContabile" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 