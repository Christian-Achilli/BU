<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c='urn:import:com.google.gwt.user.cellview.client'
  xmlns:t='urn:import:com.kp.malice.client.amministrazione'>

  <ui:style>
    .browser {
      border: 1px solid #ccc;
      float: left;
    }
    .infoForm {
      height: 400px;
      border: 1px solid #ccc;
      float: left;
    }
  </ui:style>

  <g:HTMLPanel>
    <g:FlowPanel ui:field='cellBrowserHandle' addStyleNames='{style.browser}'/>
        <t:AgentForm ui:field="agentForm" addStyleNames='{style.infoForm}'/>
        <t:AgencyForm ui:field="agencyForm" addStyleNames='{style.infoForm}'/>
    <br />
  </g:HTMLPanel>
</ui:UiBinder>