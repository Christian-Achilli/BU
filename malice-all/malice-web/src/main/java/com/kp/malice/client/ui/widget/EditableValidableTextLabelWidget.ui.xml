	<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:f="urn:import:com.kp.malice.client.ui.widget" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:com.kp.malice.client.ui" xmlns:t="urn:import:com.kp.malice.client.ui.toolbar">
	<ui:with field="resources"
		type="com.kp.malice.client.ui.resources.MaliceResources"></ui:with>
	<ui:style>
		.textTextBox {
			height: 13px;
			padding: 3px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label ui:field="label"/>
		<g:TextBox ui:field="textBox" stylePrimaryName="{style.textTextBox}" />
	</g:HTMLPanel>
</ui:UiBinder>